"original":{"line":49,"column":60}},{"generated":{"line":68,"column":89},"source":"Components/LangSwitcher.js","original":{"line":49,"column":15}}],"sources":{"Components/LangSwitcher.js":"import React, { useEffect, useRef } from \"react\";\nimport { connect } from \"react-redux\";\n\nimport { setLocale, Translate } from \"react-redux-i18n\";\n\nfunction LangSwitcher({ locale, setLocale }) {\n  const handleChange = e => {\n    const value = e.target.value;\n\n    if (setLocale) setLocale(value);\n  };\n\n  return (\n    <section className=\"langSwitcher\">\n      <h3>\n        <Translate value=\"lang.language\" />\n      </h3>\n      <form onChange={handleChange}>\n        <div className=\"radio-group\">\n          <input\n            id=\"en-option\"\n            type=\"radio\"\n            name=\"lang\"\n            value=\"en\"\n            checked={locale == \"en\"}\n          />\n          <label htmlFor=\"en-option\">\n            <Translate value=\"lang.en\" />\n          </label>\n          <input\n            id=\"ru-option\"\n            type=\"radio\"\n            name=\"lang\"\n            value=\"ru\"\n            checked={locale == \"ru\"}\n          />\n          <label htmlFor=\"ru-option\">\n            <Translate value=\"lang.ru\" />\n          </label>\n        </div>\n      </form>\n    </section>\n  );\n}\n\nconst mapStateToProps = ({ i18n }) => ({ locale: i18n.locale });\nconst mapDispatchToProps = { setLocale };\n\nexport default connect(mapStateToProps, mapDispatchToProps)(LangSwitcher);\n"},"lineCount":null}},"error":null,"hash":"e413cd378d55f4d0c8d4059745701bbd","cacheData":{"env":{}}}